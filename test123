        if (typeof MailPoet !== 'undefined') {
            MailPoet.I18n.add('ajaxFailedErrorMessage', 'An error has happened while performing a request, please try again later.')
        } else {
            setTimeout(initMailpoetTranslation, 250);
        }
    }
